{
  "name": "sources",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev":
      "concurrently \"nodemon ./bin/www\" \"parcel watch src/views/index.html --out-dir src/views/static --public-url ./\" \"cpx \"src/views/login.{html,css}\" src/views/static --watch\"",
    "createData": "node ./scripts/createData.js"
  },
  "nodemonConfig": {
    "ignore": ["src/views/*"],
    "delay": "2500"
  },
  "eslintConfig": {
    "extends": "uclaradio",
    "rules": {
      "no-debugger": 0
    }
  },
  "stylelint": {
    "extends": "stylelint-config-uclaradio"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.js",
        "options": {
          "singleQuote": true,
          "trailingComma": "es5"
        }
      }
    ]
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.0.4",
    "apollo-link-http": "^1.3.1",
    "apollo-server-express": "^1.3.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "cpx": "^1.5.0",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "googleapis": "^23.0.0",
    "graphql": "0.11.7",
    "graphql-tag": "^2.6.0",
    "graphql-tools": "^2.13.0",
    "match-sorter": "^2.2.0",
    "morgan": "~1.9.0",
    "mysql2": "^1.5.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-table": "^6.7.5",
    "sequelize": "^4.22.6",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-uclaradio": "^1.0.17",
    "faker": "^4.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.1",
    "parcel-bundler": "~1.2.1",
    "prettier": "^1.8.2",
    "style-loader": "^0.19.1",
    "stylelint": "^8.2.0",
    "stylelint-config-uclaradio": "^1.0.2",
    "webpack": "^3.10.0"
  }
}
