{
  "name": "sources",
  "version": "0.0.0",
  "private": true,
  "browserslist": [
    ">5%"
  ],
  "scripts": {
    "prestart": "tsc && yarn static-files && parcel build src/views/index.html --out-dir ./dist/views/static",
    "start": "node ./bin/www",
    "dev": "concurrently 'nodemon' 'parcel watch src/views/index.html --no-autoinstall --out-dir ./dist/views/static' 'yarn static-files:watch' --names 'üíª,‚öõÔ∏è,üì¶' --prefix name",
    "test": "NODE_ENV='test' jest --forceExit",
    "create-data": "ts-node ./scripts/create-data.ts",
    "static-files": "ts-node ./scripts/copy-static-files.ts",
    "static-files:watch": "ts-node ./scripts/copy-static-files.ts watch"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ignore": [
      "src/views/"
    ],
    "ext": "ts",
    "exec": "tsc && node ./bin/www"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-debugger": 0
    }
  },
  "prettier": {
    "editorconfig": true,
    "overrides": [
      {
        "files": "*.{js,jsx,ts,tsx}",
        "options": {
          "singleQuote": true,
          "trailingComma": "es5"
        }
      }
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/src/**/*.test.(js|ts)"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageReporters": [
      "json"
    ]
  },
  "dependencies": {
    "@types/cpx": "1.5.0",
    "@types/serve-favicon": "2.2.30",
    "apollo-cache-inmemory": "1.3.8",
    "apollo-client": "2.4.5",
    "apollo-link-http": "1.5.5",
    "apollo-server-express": "1.4.0",
    "body-parser": "1.18.3",
    "concurrently": "3.6.1",
    "connect-session-sequelize": "5.2.2",
    "cookie-parser": "1.4.3",
    "cpx": "1.5.0",
    "debug": "3.2.6",
    "dotenv": "5.0.1",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "faker": "4.1.0",
    "glamor": "2.20.40",
    "glamorous": "4.13.1",
    "global": "4.3.2",
    "googleapis": "30.0.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.10.0",
    "graphql-tools": "3.1.1",
    "match-sorter": "2.3.0",
    "morgan": "1.9.1",
    "passport": "0.4.0",
    "passport-google-oauth": "1.0.0",
    "pg": "7.6.0",
    "prop-types": "15.6.2",
    "react": "16.6.0",
    "react-apollo": "2.2.4",
    "react-contextmenu": "2.9.4",
    "react-dom": "16.6.0",
    "react-modal": "3.6.1",
    "react-table": "6.8.6",
    "sequelize": "4.41.0",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.1",
    "@types/dotenv": "4.0.3",
    "@types/express": "4.16.0",
    "@types/faker": "4.1.4",
    "@types/graphql": "0.13.4",
    "@types/jest": "23.3.9",
    "@types/morgan": "1.7.35",
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.9",
    "@types/sequelize": "4.27.29",
    "@types/supertest": "2.0.6",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "codecov": "3.1.0",
    "css-loader": "0.28.11",
    "husky": "0.14.3",
    "jest": "22.4.4",
    "lint-staged": "7.3.0",
    "node-sass": "4.9.4",
    "nodemon": "1.18.5",
    "parcel-bundler": "1.10.3",
    "prettier": "1.14.3",
    "stylelint": "9.7.0",
    "supertest": "3.3.0",
    "ts-jest": "22.4.6",
    "ts-node": "6.2.0",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-microsoft-contrib": "5.2.1",
    "tslint-react": "3.6.0",
    "tsutils": "2.29.0",
    "typescript": "2.9.2"
  }
}
